	//13. Pedir el radio de un círculo y calcular su área. A=PI*r^2.
	
	//Proceso 
	Definir r Como Real;
	Definir area Como Real;
	//Proceso 
	Escribir "Digite la longitud del radio";
	Leer r;
	
	//Proceso 
	area = PI*(r*r);
	
	//Salida
	Escribir "El área del círculo es: ", area;
FinProceso

