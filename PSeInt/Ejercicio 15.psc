Proceso  sin_titulo
	//Pedir el lado de un cuadrado, mostrar su área y su perímetro.
	
	//Proceso 
	Definir l Como Real;
	Definir area Como Real;
	Definir perimetro Como Real;
	//Proceso 
	Escribir "Digite la longitud del lado";
	Leer l;
	
	//Proceso 
	area = l*l;
	perimetro = 4*l; 
	
	//Salida
	Escribir "El área del cuadrado es: ", area;
	Escribir "El perímetro del cuadrado es : ", perimetro;
FinProceso

